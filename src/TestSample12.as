package {	import flash.display.Sprite;		import com.gamemeal.html.Canvas;	/**
	 * @author Colin
	 */
	public class TestSample12 {		public function TestSample12(root:Sprite):void{			var canvas:Canvas = new Canvas("myCanvas",165,165);			var ctx:Canvas = canvas.getContext("2d");	ctx.beginPath();	ctx.arc(100,100,30,0,Math.PI,true)	ctx.stroke();	ctx.beginPath();	ctx.moveTo(50,50);	ctx.lineTo(100,100);	ctx.arc(50,50,30,0,Math.PI,true)	ctx.stroke();	ctx.beginPath();	ctx.moveTo(0,0);	ctx.lineTo(160,0);	ctx.lineTo(160,160);	ctx.lineTo(0,160);	ctx.closePath();	ctx.stroke();	ctx.translate(20,20);	ctx.rotate(Math.PI/3);	ctx.beginPath();	ctx.fillStyle = "#000000";	ctx.moveTo(50,0);	ctx.lineTo(100,60);	ctx.lineTo(0,60);	ctx.closePath();	ctx.fill();	ctx.beginPath();	ctx.strokeStyle="#FF0000";	ctx.moveTo(-10,-10);	ctx.lineTo(10,10);	ctx.moveTo(-10,10);	ctx.lineTo(10,-10);	ctx.stroke();	ctx.beginPath();	ctx.strokeStyle="#0000FF";	ctx.moveTo(-10,-10);	ctx.lineTo(10,10);	ctx.moveTo(-10,10);	ctx.lineTo(10,-10);	ctx.stroke();	ctx.beginPath();	ctx.moveTo(0,0)	ctx.arc(0,0,30,0,Math.PI,false);	ctx.stroke(); 			root.addChild(ctx);		}	}}