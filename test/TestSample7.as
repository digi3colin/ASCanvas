package {	import flash.display.Sprite;		import com.gamemeal.html.Canvas;	/**
	 * @author Colin
	 */
	public class TestSample7 {		public function TestSample7(root:Sprite):void{			var canvas:Canvas = new Canvas("myCanvas",160,160);			var ctx:Canvas = canvas.getContext("2d");			// Quadratric curves example			ctx.beginPath();			ctx.moveTo(75,25);			ctx.quadraticCurveTo(25,25,25,62.5);			ctx.quadraticCurveTo(25,100,50,100);			ctx.quadraticCurveTo(50,120,30,125);			ctx.quadraticCurveTo(60,120,65,100);			ctx.quadraticCurveTo(125,100,125,62.5);			ctx.quadraticCurveTo(125,25,75,25);			ctx.stroke();			root.addChild(ctx);		}	}}