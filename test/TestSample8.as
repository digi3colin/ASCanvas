package {	import flash.display.Sprite;		import com.gamemeal.html.Canvas;	/**
	 * @author Colin
	 */
	public class TestSample8 {		public function TestSample8(root:Sprite):void{			var canvas:Canvas = new Canvas("myCanvas",160,160);			var ctx:Canvas = canvas.getContext("2d");		    ctx.beginPath();		    ctx.moveTo(75,40);		    ctx.bezierCurveTo(75,37,70,25,50,25);		    ctx.bezierCurveTo(20,25,20,62.5,20,62.5);		    ctx.bezierCurveTo(20,80,40,102,75,120);		    ctx.bezierCurveTo(110,102,130,80,130,62.5);		    ctx.bezierCurveTo(130,62.5,130,25,100,25);		    ctx.bezierCurveTo(85,25,75,37,75,40);		    ctx.fill();			root.addChild(ctx);		}	}}