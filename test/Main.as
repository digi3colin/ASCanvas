package {	import flash.display.MovieClip;	import flash.display.Sprite;	/**	 * @author Colin	 */	public class Main extends MovieClip{		private var sprites:Vector.<Sprite>;		public function Main(){			sprites = new Vector.<Sprite>();			this.stage.align = "TL";			for(var i:int=0;i<14;i++){				sprites[i]=new Sprite();				sprites[i].x = i%4*160;				sprites[i].y = Math.floor(i/4)*200;				this.addChild(sprites[i]);			}			new TestSample16(sprites[0]);			new TestSample(sprites[0]);			new TestSample2(sprites[1]);			new TestSample3(sprites[2]);			new TestSample4(sprites[3]);			new TestSample5(sprites[4]);			new TestSample6(sprites[5]);			new TestSample7(sprites[6]);			new TestSample8(sprites[7]);			new TestSample9(sprites[8]);			new TestSample10(sprites[9]);			new TestSample11(sprites[10]);			new TestSample13(sprites[11]);			new TestSample12(sprites[12]);		}	}}